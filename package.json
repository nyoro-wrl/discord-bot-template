{
  "name": "discord-bot-template",
  "version": "1.0.0",
  "description": "Discord bot with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "start": "npm run build && cross-env NODE_ENV=production node dist/deploy-commands.js && node dist/index.js",
    "dev": "npm run build && cross-env NODE_ENV=development node dist/deploy-commands.js && node dist/index.js"
  },
  "dependencies": {
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "cross-env": "^7.0.3",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
